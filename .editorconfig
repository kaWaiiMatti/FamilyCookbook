root = true

[*.{cs,cshtml,css,html,js,jsx,ts,tsx}]
charset = utf-8
end_of_line = lf
indent_style = space
insert_final_newline = true

[*.{cshtml,css,html,js,jsx,ts,tsx}]
indent_size = 2

[*.cs]
indent_size = 4
csharp_style_prefer_primary_constructors = false
csharp_style_namespace_declarations = file_scoped
